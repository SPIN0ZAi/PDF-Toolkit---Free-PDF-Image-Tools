# PDF Toolkit - Environment Variables
# This app runs entirely client-side, so no backend env vars needed!

# Optional: Analytics (if you add later)
# NEXT_PUBLIC_ANALYTICS_ID=

# Optional: Enable debug logging in development
NEXT_PUBLIC_DEBUG=false

# Max file size in MB (enforced client-side)
NEXT_PUBLIC_MAX_FILE_SIZE_MB=50

# Rate limiting (requests per hour per user, stored in localStorage)
NEXT_PUBLIC_RATE_LIMIT_PER_HOUR=100
